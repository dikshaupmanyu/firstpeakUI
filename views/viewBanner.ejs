  <script type="text/javascript">

window.history.forward(); 
        function noBack() { 
            window.history.forward(); 
        } 
 var url_string = window.location.href; 
  var urls = new URL(url_string);
  var tokens = urls.searchParams.get("tokendata");

  $(function(){
      var bannerIdd = $('#bannerIds').val();
       // alert(bannerIdd);
  
         $.ajax({
              type: 'POST',
              url: 'https://apis.tradetipsapp.com/api/adBannerSimple/getSimpleAdBannerById',
               headers: {
              Authorization: 'Bearer '+ tokens,
                 },
              data: {
                  adBannerId : bannerIdd,
                },
            success: function(data) {
               const dataResult = data;             
               // alert(dataResult.isSingleBanner); 

                if(dataResult.isSingleBanner == true){

                   $('.singleBannerCss').css('display','block');

                   $('.multipleBannerCss').css('display','none');

                   $('.singleBannerCssName').css('display','block');
                   
                   $('.multipleBannerCssName').css('display','none');

                     }else{

                    $('.singleBannerCss').css('display','none');
                   $('.multipleBannerCss').css('display','block');
                     $('.singleBannerCssName').css('display','none');
                   $('.multipleBannerCssName').css('display','block');

                     }
                  
                  // alert(JSON.stringify(dataResult));
                  document.getElementById("title").innerHTML = dataResult.title;
                  document.getElementById("title1").innerHTML = dataResult.title1;
                  document.getElementById("title2").innerHTML = dataResult.title2;
                  document.getElementById("title3").innerHTML = dataResult.title3;
                  document.getElementById("shortdescription").innerHTML = dataResult.shortDescription;
                  document.getElementById("shortdescription1").innerHTML = dataResult.shortDescription1;
                  document.getElementById("shortdescription2").innerHTML = dataResult.shortDescription2;
                  document.getElementById("shortdescription3").innerHTML = dataResult.shortDescription3;
                  document.getElementById("longdescription").innerHTML = dataResult.longDescription;
                  document.getElementById("longdescription1").innerHTML = dataResult.longDescription1;
                  document.getElementById("longdescription2").innerHTML = dataResult.longDescription2;
                  document.getElementById("longdescription3").innerHTML = dataResult.longDescription3;
                  document.getElementById("newImage").src = "https://apis.tradetipsapp.com"+dataResult.image;
                  document.getElementById("newImageurl").href = "https://apis.tradetipsapp.com"+dataResult.image;
                  document.getElementById("newImage1").src = "https://apis.tradetipsapp.com"+dataResult.image1;
                  document.getElementById("newImage1url").href = "https://apis.tradetipsapp.com"+dataResult.image1;
                  document.getElementById("newImage2").src = "https://apis.tradetipsapp.com"+dataResult.image2;  
                  document.getElementById("newImage2url").href = "https://apis.tradetipsapp.com"+dataResult.image2;                 
                  document.getElementById("newImage3").src = "https://apis.tradetipsapp.com"+dataResult.image3;
                  document.getElementById("newImage3url").href = "https://apis.tradetipsapp.com"+dataResult.image3;
                 

              }

            });


  });




