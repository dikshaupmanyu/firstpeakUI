  <script type="text/javascript">

window.history.forward(); 
        function noBack() { 
            window.history.forward(); 
        } 

window.addEventListener("load", function() {

  var url_string = window.location.href; 
  var urls = new URL(url_string);
  var tokens = urls.searchParams.get("tokendata");
  var formdata = new FormData();
  formdata.append("offset", "0");
  formdata.append("limit", "500");

  var requestOptions = {
    method: 'POST',
    headers: {
          Authorization: 'Bearer ' + tokens,
        },
    body: formdata,
    redirect: 'follow'
  };

  fetch("https://apis.tradetipsapp.com/api/appUserWebModify/getAllAppUsers", requestOptions)
  .then(resp => resp.json())
    .then(function(data) {
      // console.log(data);
      for( var i = 0; i< data.length; i++){
          var userid = data[i]['id'];
          var username = data[i]['userName'];
          $("#userFull").append("<option value='"+userid+"'>"+username+"</option>");
       }         
  });
 });   
   
 </script>
 
